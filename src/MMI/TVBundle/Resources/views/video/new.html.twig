{% extends '::layout.html.twig' %}
{% block title %}
    {{ parent() }} Ajout d'une vidéo
{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
                <h1>Ajouter une vidéo</h1>

                <div class="form-group">
                    {% form_theme form "form.html.twig" %}
                    {{ form_start(form) }}
                    {{ form_end(form) }}
                    <input type="submit" value="Ajouter une vidéo" />
                </div>

                <ul>
                    <li>
                        <a href="{{ path('mmitv_home') }}">Retour à la liste de programmation</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>


{% endblock %}

